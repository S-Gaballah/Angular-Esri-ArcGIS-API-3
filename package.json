{
  "name": "esri-javascript-v3-angular4-material-design",
  "version": "1.0.0",
  "description": "Startup Application for ArcGIS API for JavaScript with Angular 4 and material design",
  "main": "src/index.html",
  "scripts": {
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "serve": "lite-server -c=bs-config.json",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "lint": "tslint ./src/**/*.ts -t verbose"
  },
  "keywords": [
    "Esri",
    "JavaScript",
    "Angular2",
    "Angular4"
  ],
  "author": "Sherif Gaballah",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^4.3.1",
    "@angular/cdk": "^2.0.0-beta.8",
    "@angular/common": "^4.2.2",
    "@angular/compiler": "^4.2.2",
    "@angular/core": "^4.2.2",
    "@angular/flex-layout": "^2.0.0-beta.8",
    "@angular/forms": "^4.2.2",
    "@angular/http": "^4.2.2",
    "@angular/material": "^2.0.0-beta.8",
    "@angular/platform-browser": "^4.2.2",
    "@angular/platform-browser-dynamic": "^4.2.2",
    "@angular/router": "^4.2.2",
    "@material/list": "^0.2.10",
    "@types/arcgis-js-api": "3.20",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.1",
    "esri-system-js": "~1.0.0-beta.0",
    "hammerjs": "^2.0.8",
    "material-components-web": "^0.12.1",
    "rxjs": "^5.0.3",
    "systemjs": "^0.20.13",
    "systemjs-plugin-dojo": "^1.0.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "concurrently": "^3.2.0",
    "lite-server": "^2.2.2",
    "typescript": "~2.2.0",
    "canonical-path": "0.0.2",
    "tslint": "^3.15.1",
    "rimraf": "^2.5.4",
    "@types/node": "^6.0.46"
  }
}
